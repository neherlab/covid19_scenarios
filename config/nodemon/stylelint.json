{
  "quiet": true,
  "legacyWatch": false,
  "delay": 0.1,
  "watch": [
    "../.env",
    ".browserslistrc",
    ".editorconfig",
    ".env",
    ".prettierignore",
    ".prettierrc",
    "config",
    "jest.config.js",
    "package.json",
    "postcss.config.js",
    "src",
    "static",
    "stylelint.config.js",
    "yarn.lock"
  ],
  "ext": "js,jsx,ts,tsx,json,flow,css,scss,sass,styl,html,ejs,d,txt,yml,sh",
  "ignore": ["**/*.test.*s", "**/*.test.*sx", "**/__tests__", ".build", "config/jest", "node_modules"],
  "exec": "yarn clear && yarn stylelint && echo '\u2714 OK' || echo '\u274C FAILED' || cd ."
}
