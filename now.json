{
  "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }],
  "build": {
    "env": {
      "WEB_APP_NAME_FRIENDLY": "COVID-19 Scenarios",
      "WEB_DEV_SERVER_PORT": "3000",
      "WEB_DEV_BUNDLE_ANALYZER_PORT": "8888",
      "DEBUGGABLE_PROD": "1",
      "DEV_FANCY_CONSOLE": "1",
      "DEV_FANCY_CLEAR_CONSOLE": "0",
      "DEV_DISABLE_LINT": "1",
      "WEB_PROD_HOST": "neherlab.org/covid19"
    }
  }
}
