{
  "extends": [
    "tslint-consistent-codestyle",
    "tslint-config-airbnb",
    "tslint-eslint-rules",
    "tslint-microsoft-contrib/latest",
    "tslint:recommended",
    "tslint-sonarts",
    "tslint-react",
    "tslint-plugin-prettier",
    "tslint-config-prettier"
  ],
  "defaultSeverity": "warning",
  "rulesDirectory": [
    "tslint-plugin-prettier",
    "node_modules/tslint-microsoft-contrib",
    "node_modules/tslint-eslint-rules/dist/rules"
  ],
  "jsRules": {
    "array-type": false,
    "completed-docs": false,
    "export-name": false,
    "function-name": false,
    "import-name": false,
    "interface-name": false,
    "jsx-boolean-value": false,
    "jsx-no-lambda": false,
    "jsx-no-multiline-js": false,
    "jsx-wrap-multiline": false,
    "max-func-body-length": false,
    "mocha-no-side-effect-code": false,
    "no-backbone-get-set-outside-model": false,
    "no-big-function": false,
    "no-commented-code": false,
    "no-console": false,
    "no-constant-condition": false,
    "no-default-export": false,
    "no-duplicate-string": false,
    "no-empty-interface": false,
    "no-http-string": false,
    "no-implicit-dependencies": [true, "dev"],
    "no-increment-decrement": false,
    "no-parameter-reassignment": false,
    "no-relative-imports": false,
    "no-shadowed-variable": false,
    "no-single-line-block-comment": false,
    "no-submodule-imports": false,
    "no-suspicious-comment": false,
    "no-typeof-undefined": false,
    "no-useless-files": false,
    "no-var-requires": false,
    "non-literal-fs-path": false,
    "object-literal-sort-keys": false,
    "object-shorthand-properties-first": false,
    "prefer-array-literal": false,
    "prefer-for-of": false,
    "prefer-type-cast": false,
    "prettier": true,
    "react-a11y-accessible-headings": false,
    "react-this-binding-issue": false,
    "underscore-consistent-invocation": false,
    "use-simple-attributes": false
  },
  "rules": {
    "array-type": false,
    "completed-docs": false,
    "export-name": false,
    "function-name": false,
    "import-name": false,
    "interface-name": false,
    "jsx-boolean-value": false,
    "jsx-no-lambda": false,
    "jsx-no-multiline-js": false,
    "jsx-wrap-multiline": false,
    "max-func-body-length": false,
    "mocha-no-side-effect-code": false,
    "no-backbone-get-set-outside-model": false,
    "no-big-function": false,
    "no-commented-code": false,
    "no-console": false,
    "no-constant-condition": false,
    "no-default-export": false,
    "no-duplicate-string": false,
    "no-empty-interface": false,
    "no-http-string": false,
    "no-implicit-dependencies": [true, "dev"],
    "no-increment-decrement": false,
    "no-parameter-reassignment": false,
    "no-relative-imports": false,
    "no-shadowed-variable": false,
    "no-single-line-block-comment": false,
    "no-submodule-imports": false,
    "no-suspicious-comment": false,
    "no-typeof-undefined": false,
    "no-useless-files": false,
    "no-var-requires": false,
    "non-literal-fs-path": false,
    "object-literal-sort-keys": false,
    "object-shorthand-properties-first": false,
    "prefer-array-literal": false,
    "prefer-for-of": false,
    "prefer-type-cast": false,
    "prettier": true,
    "react-a11y-accessible-headings": false,
    "react-this-binding-issue": false,
    "underscore-consistent-invocation": false,
    "use-simple-attributes": false,
    "no-namespace": [true, "allow-declarations"]
  },
  "linterOptions": {
    "exclude": [
      ".build/**",
      ".cache/**",
      ".ignore/**",
      ".reports/**",
      "3rdparty/**",
      "generated/**",
      "node_modules*/**"
    ]
  }
}
